{
    "official": true,
    "merge_patches": true,
    "java_target": 21,
    "encoding": "UTF-8",
    "test_remap_official": false,
    "only_include_parameter_mappings": true,
    "no_obfuscation": true,
    "version_zip": "https://piston-data.mojang.com/v1/objects/de334d80d9ddc5abb94c611b8ad10f9125c4c421/25w45a_unobfuscated.zip",
    "fernflower": {
        "version": "org.vineflower:vineflower:1.11.2",
        "java_version": 21,
        "args": ["--decompile-inner", "--remove-bridge", "--decompile-generics", "--ascii-strings", "--remove-synthetic", "--include-classpath", "--ignore-invalid-bytecode", "--bytecode-source-mapping", "--dump-code-lines", "--indent-string=    ", "--log-level=TRACE", "-cfg", "{libraries}", "{input}", "{output}"],
        "jvmargs": ["-Xmx6G"]
    },
    "merge": {
        "version": "net.neoforged:mergetool:2.0.7:fatjar",
        "args": ["--client", "{client}", "--server", "{server}", "--ann", "API", "--output", "{output}", "--inject", "false"],
        "jvmargs": []
    },
    "bundler_extract_jar": {
        "version": "net.neoforged.installertools:installertools:3.0.14:fatjar",
        "args": ["--task", "bundler_extract", "--input", "{input}", "--output", "{output}", "--jar-only"]
    },
    "bundler_extract_libs": {
        "version": "net.neoforged.installertools:installertools:3.0.14:fatjar",
        "args": ["--task", "bundler_extract", "--input", "{input}", "--output", "{output}", "--libraries"]
    },
    "libraries": {
        "client": ["com.google.code.findbugs:jsr305:3.0.2", "ca.weblite:java-objc-bridge:1.1", "org.jetbrains:annotations:26.0.2-1"],
        "server": ["com.google.code.findbugs:jsr305:3.0.2", "org.jetbrains:annotations:26.0.2-1"],
        "joined": ["com.google.code.findbugs:jsr305:3.0.2", "ca.weblite:java-objc-bridge:1.1", "net.neoforged:mergetool:2.0.7:api", "org.jetbrains:annotations:26.0.2-1"]
    }
}
